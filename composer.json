{
  "name": "svc/video-bundle",
  "description": "Bundle to manage and stream videos",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Sven Vetter",
      "email": "git.09o@svenvetter.com"
    }
  ],
  "keywords": [
    "symfony-ux"
  ],
  "require": {
    "php": "^8.1.0",
    "symfony/framework-bundle": "^6.1",
    "doctrine/doctrine-bundle": "^2.6",
    "symfony/validator": "^6.1",
    "symfony/expression-language": "^6.1",
    "symfony/lock": "^6.1",
    "symfony/translation": "^6.1",
    "symfony/twig-bundle": "^6.1",
    "svc/like-bundle": "^4|dev-main",
    "svc/util-bundle": "^4|dev-main",
    "svc/log-bundle": "^4.2|dev-main",
    "liip/imagine-bundle": "^2.7",
    "symfony/form": "^6.1",
    "symfony/ux-chartjs": "^2.3",
    "babdev/pagerfanta-bundle": "^3.7",
    "pagerfanta/twig": "^3.6",
    "pagerfanta/doctrine-orm-adapter": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Svc\\VideoBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Svc\\VideoBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^6.1",
    "symfony/browser-kit": "^6.1",
    "phpstan/phpstan": "^1.8"
  },
  "scripts": {
    "phpstan": "php -d memory_limit=-1 vendor/bin/phpstan analyse src/ --level 5"
  }
}
