{
  "name": "svc/video-bundle",
  "description": "Bundle to manage and stream videos",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Sven Vetter",
      "email": "git.09o@svenvetter.com"
    }
  ],
  "keywords": [
    "symfony-ux"
  ],
  "require": {
    "php": "^8.0.2",
    "symfony/framework-bundle": "5.4.*|^6",
    "doctrine/doctrine-bundle": "^2.6",
    "symfony/validator": "5.4.*|^6",
    "symfony/expression-language": "5.4.*|^6",
    "symfony/translation": "5.4.*|^6",
    "symfony/twig-bundle": "5.4.*|^6",
    "svc/like-bundle": "^3|dev-main",
    "svc/util-bundle": "^3|dev-main",
    "svc/log-bundle": "^3|dev-main",
    "liip/imagine-bundle": "^2",
    "symfony/form": "5.4.*|^6",
    "symfony/ux-chartjs": "^2"
  },
  "autoload": {
    "psr-4": {
      "Svc\\VideoBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Svc\\VideoBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "symfony/phpunit-bridge": "5.4.*|^6",
    "symfony/browser-kit": "5.4.*|^6",
    "phpstan/phpstan": "^1"
  },
  "scripts": {
    "phpstan": "php -d memory_limit=-1 vendor/bin/phpstan analyse src/ --level 5"
  }
}
